<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.amherst.fyang17.myapplication" >

    <uses-sdk android:maxSdkVersion="11" />

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.amherst.fyang17.myapplication.MyActivity" />
        </activity>
        <activity
            android:name=".DisplayHistText"
            android:label="@string/title_activity_display_hist_text"
            android:parentActivityName=".MyActivity" >
        </activity>
        <activity
            android:name=".sumActivity"
            android:label="@string/title_activity_sum"
            android:parentActivityName=".MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.amherst.fyang17.myapplication.MyActivity" />
        </activity>
        <activity
            android:name=".SumActivity"
            android:label="@string/title_activity_sum"
            android:parentActivityName=".MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.amherst.fyang17.myapplication.MyActivity" />
        </activity>
    </application>

</manifest>
